{
  "compilerOptions": {
    "baseUrl": ".",
    "declaration": true,
    "esModuleInterop": true,
    "jsx": "react",
    "lib": [ "es5", "es2015", "es2017" ],
    "module": "commonjs",
    "moduleResolution": "node",
    "noImplicitAny": true,
    "noImplicitThis": false,
    "strict": true,
    "target": "ES6"
  },
  "exclude": [
    "./node_modules/*"
  ]
}
